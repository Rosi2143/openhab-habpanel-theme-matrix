{
    "template": "<div class=\"section\">\n\n  \t<div class=\"bigDash\">\n    <div class=\"top\">\n\t\t\t<div class=\"icon on\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#thermometer-3\"></use></svg></div>\n\t\t\t<div class=\"value\">\n\t\t\t\t<div class=\"main\">{{itemValue('localCurrentTemperature').split(' ')[0] | number:1}}</div>\n\t\t\t\t<div class=\"sub\">&#176C</div>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"bottom\">\n\t\t\t<div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#thermometer-3\"></use></svg></div>\n\t\t\t<div class=\"value\">feels like {{itemValue('temperature_0d_feel').split(' ')[0] | number:0}} &#176C</div>\n\t\t\t<div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#drop\"></use></svg></div>\n\t\t\t<div class=\"value\">{{itemValue('localCurrentHumidity').split(' ')[0] | number:0}} %</div>\n\t\t</div>\n\t</div>\n  \n\t<div class=\"bigDash\">\n\t\t<div class=\"top\">\n\t\t\t<div class=\"icon on\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#umbrella\"></use></svg></div>\n\t\t\t<div class=\"value\">\n\t\t\t\t<div class=\"main\">{{itemValue('localCurrentRainVolume').split(' ')[0] | number:1}}</div>\n\t\t\t\t<div class=\"sub\">mm</div>\n\t\t\t</div>\n\t\t</div>\n\t\t<div class=\"bottom\">\n\t\t\t<div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#umbrella\"></use></svg></div>\n\t\t\t<div class=\"value\">1h: {{itemValue('rain_0d_1h') | number:0}} mm</div>\n\t\t\t<div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/squidink.svg#umbrella\"></use></svg></div>\n\t\t\t<div class=\"value\">12h: {{itemValue('rain_0d_12h') | number:0}} mm</div>\n\t\t</div>\n\t</div>  \n\n  <div class=\"bigDash\">\n\t\t<div class=\"top\">\n      <div class=\"icon on wind\"><div style=\"transform: rotate({{itemValue('loca