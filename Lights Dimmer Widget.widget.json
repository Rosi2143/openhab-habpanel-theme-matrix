{
    "template": " \n<div class=\"section\" ng-init=\"hueColors = [ { hsb: '0,0,100', hex: '#fff' }, { hsb: '74,78,100', hex: '#fecc2f' }, { hsb: '46,100,100', hex: '#f9a228' }, { hsb: '26,100,100', hex: '#f6621f' }, { hsb: '0,100,100', hex: '#db3838' }, { hsb: '273,100,100', hex: '#a363d9' }, { hsb: '201,100,100', hex: '#40a4d8' }, { hsb: '177,100,100', hex: '#33beb8' }, { hsb: '140,100,100', hex: '#b2c225' } ]\">\n  <div ng-repeat=\"group in itemsInGroup('fgHABPanelWidgetItems_DimmerLights') | orderBy:'name':'reverse'\">\n\n    <div class=\"sectionIconContainer\" ng-if=\"itemValue(group.name)=='OFF'\" ng-click=\"sendCmd(group.name, 'ON')\">\n      <div class=\"sectionIcon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#light_bulb\"></use></svg></div>\n    </div>\n    <div class=\"sectionIconContainer\" ng-if=\"itemValue(group.name)=='ON'\" ng-click=\"sendCmd(group.name, 'OFF')\">\n      <div class=\"sectionIcon on\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#light_bulb\"></use></svg></div>\n    </div>\n    <div class=\"sectionIconContainer\" ng-if=\"itemValue(group.name)=='NULL'\" ng-click=\"sendCmd(group.name, 'ON')\">\n      <div class=\"sectionIcon warn\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#light_bulb\"></use></svg></div>\n    </div>\n    <div class=\"title\">\n      <div class=\"name\">{{group.label}}</div>\n      <div class=\"summary\">ON: {{ ( filtered | filter: { state: 'ON' } ).length }} of {{filtered.length}}</div>\n    </div>\n    <div class=\"controls\">\n\t\n      <div ng-repeat=\"item in itemsInGroup(group.name) | orderBy:'name' | filter:query as filtered\">\n\n        <div class=\"widget\" ng-if=\"item.type=='Color' && itemValue(item.name)=='0,0,0'\" ng-click=\"showHueSelect = !showHueSelect\">\n          <div class=\"icon off\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#off\"></use></svg></div>\n          <div class=\"name\">\n            <div class=\"hue\" ng-style=\"{'background': 'rgb(' + itemValue(item.name) + ')'}\"></div>\n            <div class=\"hueSelect\" ng-init=\"showHueSelect = false\" ng-show=\"showHueSelect\">\n              <div class=\"hueSelectItem\"></div>\n              <div class=\"hueSelectOptions\">\n                <a href=\"\" ng-click=\"sendCmd(item.name, 'OFF')\"><svg viewBox=\"0 0 48 48\" style=\"stroke: white; stroke-width: 3px;\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#cross-line\"></use></svg></a>\n                <a href=\"\" ng-click=\"sendCmd(item.name, color.hsb)\" ng-repeat=\"color in hueColors\" ng-style=\"{ 'background': color.hex }\"></a>\n              </div>\n            </div>{{item.label}}\n          </div>\n        </div>\n\n        <div class=\"widget\" ng-if=\"item.type=='Color' && itemValue(item.name)!='0,0,0'\" ng-click=\"showHueSelect = !showHueSelect\">\n          <div class=\"icon on\"><svg viewBox=\"0 0 48 48\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#on\"></use></svg></div>\n          <div class=\"name\">\n            <div class=\"hue\" ng-style=\"{'background': 'rgb(' + itemValue(item.name) + ')'}\"></div>\n            <div class=\"hueSelect\" ng-init=\"showHueSelect = false\" ng-show=\"showHueSelect\">\n              <div class=\"hueSelectItem\"></div>\n              <div class=\"hueSelectOptions\">\n                <a href=\"\" ng-click=\"sendCmd(item.name, 'OFF')\"><svg viewBox=\"0 0 48 48\" style=\"stroke: white; stroke-width: 3px;\"><use xlink:href=\"/static/matrix-theme/matrixicons.svg#cross-line\"></use></svg></a>\n                <a href=\"\" ng-click=\"sendCmd(item.name, color.hsb)\" ng-repeat=\"color in hueColors\" ng-style=\"{ 'background': color.hex }\"></a>\n              </div>\n            </div>{{item.label}} <!--  - {{itemValue(item.name)}} -->\n          </div>\n        </div>\n\n        <div class=\"widget\" ng-if=\"item.type=='Dimmer' && itemValue(item.name)!='On'\" ng-click=\"sendCmd(item.name, 'ON')\">\n          <div class=\"leftborder\"></div>\n          <div class=\"leftborder-label\">{{item.label}}: Volume</div>\n          <div class=\"switch btn-group slider-leftborder\" ng-init=\"model={item: item.name, hidelabel: true, hidepointer: true, hidelimits: true }\">\n            <widget-slider ng-model=\"model\" style=\"width: 80%; margin-left: -10px; display: block;\"></widget-slider>\n          </div>\n          <div class=\"value\" style=\"width: 80%; display: block; margin-top: -12px; font-size: 14px;\">{{itemState(item.name)}}%</div>\n\n          <div class=\"leftborder\"></div>\n          <div class=\"leftborder-label\">{{item.label}}: Farbe</div>\n          <div class=\"switch btn-group slider-leftborder\" ng-init=\"model={item: item.name + '_ColorTemp', hidelabel: true, hidepointer: true, hidelimits: true }\">\n            <widget-slider ng-model=\"model\" style=\"width: 80%; margin-left: -10px; display: block;\"></widget-slider>\n          </div>\n          <div class=\"value\" style=\"width: 80%; display: block; margin-top: -12px; font-size: 14px;\">{{itemState(item.name + '_ColorTemp')}}%</div>\n        </div>\n\n        <!-- {{item.label}} -- {{item.type}} -- {{item.state}} -- {{itemValue(item.name)}} -->\n      </div>\n\n    </div>\n\t</div>\n</div>"
}